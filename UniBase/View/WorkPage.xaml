                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>

                    <GridView ItemsSource="{Binding ManageUser.Users}" SelectedItem="{Binding ManageUser.SelectedUser}" Margin="10,10,10,0">
                        <GridView.ItemTemplate>
                            <DataTemplate x:DataType="model:ManageUser">
                                <StackPanel Orientation="Horizontal" Margin="10">
                                    <Image Source="{Binding ImageTb}" Width="100" Height="100"/>
                                    <StackPanel Orientation="Vertical">
                                        <TextBlock Width="160" FontWeight="Bold" Text="{Binding Name}"/>
                                        <StackPanel Orientation="Horizontal">
                                            <TextBlock Foreground="{Binding ForegroundColor}" Text="{Binding}" HorizontalAlignment="Center"/>
                                            <TextBlock Foreground="{Binding ForegroundColor}" Text="stk"/>
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal">
                                            <TextBlock Text="Indkøbspris: kr."/>
                                            <TextBlock Text="{Binding BuyingPrice}" Margin="2,0,2,0"/>
                                            <TextBlock Text=",-"/>
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal">
                                            <TextBlock Text="Salgspris: kr."/>
                                            <TextBlock Text="{Binding SellingPrice}" Margin="2,0,2,0"/>
                                            <TextBlock Text=",-"/>
                                        </StackPanel>

                                    </StackPanel>
                                </StackPanel>
                            </DataTemplate>
                        </GridView.ItemTemplate>
                    </GridView>
                    <Rectangle Fill="#16503F" Grid.Row="1" Margin="-11,-10,-11,0"/>
                    <Grid Grid.Row="1" Margin="10,0,10,0">
       
                            <StackPanel VerticalAlignment="Center" HorizontalAlignment="Left">
                            <Button HorizontalAlignment="Center" Content="Tilføj bruger" Visibility="Visible" Command="{Binding AddUserCommand}"/>
                            <Button HorizontalAlignment="Center" Content="Slet bruger" Visibility="Visible" Command="{Binding DeleteUserCommand}" Margin="0,5,0,0"/>
                        </StackPanel>
                        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
                            
                            <TextBox PlaceholderText="Name" Visibility="Visible" Text="{Binding ManageUser.NameTb}"></TextBox>
                            <TextBox PlaceholderText="Email" Visibility="Visible" Text="{Binding ManageUser.EmailTb}"></TextBox>
                            <TextBox PlaceholderText="TelephoneNumber" Visibility="Visible" Text="{Binding ManageUser.TelephoneNumberTb}"></TextBox>
                            <PasswordBox PlaceholderText="Password" Visibility="Visible" Password="{Binding ManageUser.PasswordTb}"></PasswordBox>
                            <PasswordBox PlaceholderText="Confirm password" Visibility="Visible" Password="{Binding ManageUser.ConfirmPasswordTb}"></PasswordBox>
                        </StackPanel>
                        <Button HorizontalAlignment="Right" Content="Ændre bruger" Visibility="Visible" Command="{Binding}"/>
                    </Grid>

                </Grid>

            </PivotItem>
        </Pivot>
    </Grid>
</Page>
