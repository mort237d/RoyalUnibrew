                                <Button Style="{StaticResource ButtonTransparentHoverStyle}" ToolTipService.ToolTip="Tilføj" Command="{Binding AddFrontpageTable}">
                                    <Image Source="Images/AddButton.png"></Image>
                                </Button>
                            </StackPanel>
                        </StackPanel>
                    </PivotItem>
