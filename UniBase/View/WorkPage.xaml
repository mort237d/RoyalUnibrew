                                </StackPanel>
                                <Button Style="{StaticResource ButtonTransparentHoverStyle}" Height="40" Width="40" ToolTipService.ToolTip="Tilføj" Command="{Binding AddTUTable}">
                                    <Image Source="Images/AddButton.png"/>
                                </Button>
                            </StackPanel>
                        </StackPanel>
                    </PivotItem>
                    <!--#endregion-->
                </Pivot>
            </PivotItem>
            <!--#endregion-->
            <PivotItem Header="K3"/>
            <PivotItem Header="K4"/>
            <PivotItem Header="K5"/>
            <PivotItem Header="Grafer" Visibility="Visible" >
                <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                    <StackPanel Orientation="Horizontal"  Margin="3">
                        <TextBlock FontWeight="SemiBold" Text="Vælg type graf" Margin="5,0,105,0"/>
                        <TextBlock FontWeight="SemiBold" Text="Vælg tidsperiode"/>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <ComboBox  x:Name="SelectGraphCombobox" PlaceholderText="Vægt" Width="200" SelectedItem="{Binding Column_2.TrendAdminstrator.GraphType, Mode=TwoWay}">
                            <ComboBoxItem Content="Vægt"/>
                            <ComboBoxItem Content="MipMa"/>
                            <ComboBoxItem Content="Lud Koncentration"/>
                        </ComboBox>

                        <ComboBox x:Name="SelectedGraphTimeperiodCB" PlaceholderText="En Uge" Width="200" SelectedItem="{Binding Column_2.TrendAdminstrator.GraphTimePeriod, Mode=TwoWay}">
                            <ComboBoxItem Content="En Uge"/>
                            <ComboBoxItem Content="En Måned"/>
                            <ComboBoxItem Content="Et Kvartal"/>
                            <ComboBoxItem Content="Et År"/>

                        </ComboBox>
                    </StackPanel>
                    <TextBlock x:Name="TbCombobox" Text="{Binding GraphCombobox, Mode=TwoWay}" Visibility="Collapsed"/>
                    <TextBlock x:Name="TbTimeperiodCombobox" Text="{Binding GraphTimeperiodCombobox, Mode=TwoWay}" Visibility="Collapsed"/>

                    <charting:Chart  x:Name="LineChart" HorizontalAlignment="Center" Margin="5" Width="1100" Height="500"  >
                        <charting:LineSeries Title="{Binding GraphCombobox, Mode=TwoWay}" 
                                                 IndependentValuePath="Date"
                                                 DependentValuePath="SelectedCategory"
                                                 ItemsSource="{Binding Column_2.TrendAdminstrator.TrendList, Mode=TwoWay}"/>
                        <charting:LineSeries Title="Min værdi" 
                                                 IndependentValuePath="Date"
                                                 DependentValuePath="MinValue"
                                                 ItemsSource="{Binding Column_2.TrendAdminstrator.TrendList, Mode=TwoWay}"/>
                        <charting:LineSeries Title="Max værdi" 
                                                 IndependentValuePath="Date"
                                                 DependentValuePath="MaxValue"
                                                 ItemsSource="{Binding Column_2.TrendAdminstrator.TrendList, Mode=TwoWay}"/>
                    </charting:Chart>



                </StackPanel>

            </PivotItem>
            <PivotItem Header="Brugere">

                <StackPanel Orientation="Horizontal">
                    <GridView ItemsSource="{Binding ManageUser.Users}" SelectedItem="{Binding ManageUser.SelectedUser, Mode=TwoWay}" Margin="10,10,10,0">
                        <GridView.ItemTemplate>
                            <DataTemplate x:DataType="model:ManageUser">
                                <StackPanel Orientation="Horizontal" Margin="10">
                                    <Image Source="{Binding ImageTb}" Width="100" Height="100"/>
                                    <StackPanel>
                                        <Image Height="60" Width="40" Source="Images/AddButton.png"></Image>
                                        <TextBlock Width="160" FontWeight="Bold" Text="{Binding NameTb, Mode=TwoWay}"/>
                                        <StackPanel Orientation="Horizontal">
                                            <TextBlock Text="{Binding EmailTb, Mode=TwoWay}"/>
                                            <TextBlock Text="{Binding TelephoneNumberTb, Mode=TwoWay}"/>
                                        </StackPanel>

                                    </StackPanel>
                                </StackPanel>
                            </DataTemplate>
                        </GridView.ItemTemplate>
                    </GridView>
                    <StackPanel>
                        <TextBox PlaceholderText="Name" Visibility="Visible" Text="{Binding ManageUser.NameTb, Mode=TwoWay}"></TextBox>
                        <TextBox PlaceholderText="Email" Visibility="Visible" Text="{Binding ManageUser.EmailTb, Mode=TwoWay}"></TextBox>
                        <TextBox PlaceholderText="TelephoneNumber" Visibility="Visible" Text="{Binding ManageUser.TelephoneNumberTb, Mode=TwoWay}"></TextBox>
                        <PasswordBox PlaceholderText="Password" Visibility="Visible" Password="{Binding ManageUser.PasswordTb, Mode=TwoWay}"></PasswordBox>
                        <PasswordBox PlaceholderText="Confirm password" Visibility="Visible" Password="{Binding ManageUser.ConfirmPasswordTb, Mode=TwoWay}"></PasswordBox>
                        <TextBox PlaceholderText="Billedetest" Visibility="Visible" Text="{Binding ManageUser.ImageTb, Mode=TwoWay}"></TextBox>
                        <Button HorizontalAlignment="Left" Content="Tilføj bruger" Visibility="Visible" Command="{Binding AddUserCommand, Mode=TwoWay}" Margin="0,0,0,0" VerticalAlignment="Center" Width="135"/>
                        <Button HorizontalAlignment="Left" Content="Slet bruger" Visibility="Visible" Command="{Binding DeleteUserCommand}" Margin="0,0,0,0" VerticalAlignment="Center" Width="135"/>
                        <Button HorizontalAlignment="Center" Content="Ændre bruger" Visibility="Visible" Command="{Binding}" Margin="0,0,0,0" VerticalAlignment="Center" Width="135"/>
                    </StackPanel>

                </StackPanel>

            </PivotItem>
        </Pivot>
    </Grid>
</Page>
